{
	"compilerOptions": {
		"target": "es2021",
		"module": "commonjs",
		"allowJs": true,
		"outDir": "dist",
		"moduleResolution": "node",
		"baseUrl": "src",
		"sourceMap": true,
		"paths": {
			"#classes/*": ["classes/*"],
			"#handlers/*": ["classes/Handlers/*"],
			"#functions/*": ["functions/*"],
			"#utils/*": ["utils/*"],
			"#client": ["classes/Ryneczek.ts"],
			"#config": ["../config.json"]
		},
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true
	}
}

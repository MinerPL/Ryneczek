{
	"name": "ryneczek",
	"version": "1.0.0",
	"main": "dist",
	"type": "module",
	"author": "Dawid Jaworski <contact@minerpl.xyz>",
	"license": "MIT",
	"scripts": {
		"dev": "npm run build && npm start",
		"build": "rm -rf dist/ && tsc",
		"start": "npm run migrate:deploy && node --enable-source-maps .",
		"migrate:reset": "prisma migrate reset",
		"migrate:create": "prisma migrate dev --create-only",
		"migrate:dev": "prisma migrate dev",
		"migrate:deploy": "prisma migrate deploy",
		"prisma:generate": "prisma generate"
	},
	"imports": {
		"#classes/*": "./dist/classes/*.js",
		"#handlers/*": "./dist/classes/Handlers/*.js",
		"#functions/*": "./dist/functions/*.js",
		"#utils/*": "./dist/utils/*.js",
		"#client": "./dist/classes/Ryneczek.js",
		"#prisma": "./prisma/generated/prisma/client.js",
		"#types/*": "./dist/types/*.js"
	},
	"dependencies": {
		"@prisma/client": "^6.16.2",
		"@sentry/node": "^10.15.0",
		"discord-api-types": "^0.38.26",
		"discord-html-transcripts": "^3.3.0",
		"discord.js": "^14.22.1",
		"undici": "^7.10.0"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.2.4",
		"prisma": "^6.16.2",
		"typescript": "^5.9.2"
	}
}
